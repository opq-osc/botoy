{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "FriendMsg",
  "type": "object",
  "properties": {
    "CurrentPacket": {
      "type": "object",
      "properties": {
        "EventData": {
          "type": "object",
          "properties": {
            "MsgHead": {
              "type": "object",
              "properties": {
                "FromUin": { "type": "integer" },
                "ToUin": { "type": "integer" },
                "FromType": {
                  "type": "integer",
                  "enum": [1, 2, 3],
                  "description": "消息来源类型 3私聊 2群组 1好友"
                },
                "SenderUin": { "type": "integer", "description": "发送者QQ号" },
                "SenderNick": { "type": "string" },
                "MsgType": { "type": "integer" },
                "C2cCmd": {
                  "type": "integer",
                  "enum": [0, 1, 17, 349, 20, 212, 8, 11],
                  "description": "0 收到群消息, 1 发出去消息的回应, 17 群消息被撤回, 349 上下线, 20 被拉群， 212 群解散， 8 上线， 11 好友私聊 529 手机"
                },
                "MsgSeq": { "type": "integer" },
                "MsgTime": { "type": "integer" },
                "MsgRandom": { "type": "integer" },
                "MsgUid": { "type": "integer" },
                "GroupInfo": {
                  "type": "object",
                  "properties": {
                    "GroupCard": { "type": "string" },
                    "GroupCode": { "type": "integer" },
                    "GroupInfoSeq": { "type": "integer" },
                    "GroupLevel": { "type": "integer" },
                    "GroupRank": { "type": "integer" },
                    "GroupType": { "type": "integer" },
                    "GroupName": { "type": "string" }
                  },
                  "required": [
                    "GroupCard",
                    "GroupCode",
                    "GroupInfoSeq",
                    "GroupLevel",
                    "GroupRank",
                    "GroupType",
                    "GroupName"
                  ]
                },
                "C2CTempMessageHead": {
                  "type": "object",
                  "properties": {
                    "C2CType": { "type": "integer" },
                    "Sig": { "type": "string" },
                    "GroupUin": { "type": "integer" },
                    "GroupCode": { "type": "integer" }
                  },
                  "required": ["C2CType", "Sig", "GroupCode", "GroupUin"]
                }
              },
              "required": [
                "FromUin",
                "ToUin",
                "FromType",
                "SenderUin",
                "SenderNick",
                "MsgType",
                "C2cCmd",
                "MsgSeq",
                "MsgTime",
                "MsgRandom",
                "MsgUid"
              ]
            },
            "MsgBody": {
              "type": "object",
              "required": ["Content", "SubMsgType"],
              "properties": {
                "SubMsgType": {
                  "type": "integer",
                  "description": "0为单一或复合类型消息(文字 At 图片 自由组合), 12 Xml消息 19 Video消息 51 JSON卡片消息"
                },
                "Content": { "type": "string" },
                "AtUinLists": {
                  "type": "array",
                  "items": {
                    "type": "object",
                    "properties": {
                      "Nick": { "type": "string" },
                      "Uin": { "type": "integer" }
                    },
                    "required": ["Uin", "Nick"]
                  }
                },
                "Images": {
                  "type": "array",
                  "items": {
                    "type": "object",
                    "properties": {
                      "FileId": { "type": "integer" },
                      "FileMd5": { "type": "string" },
                      "FileSize": { "type": "integer" },
                      "Url": { "type": "string" }
                    },
                    "required": ["FileId", "FileMd5", "FileSize", "Url"]
                  }
                },
                "Video": {
                  "type": "object",
                  "properties": {
                    "FileMd5": { "type": "string" },
                    "FileSize": { "type": "integer" },
                    "Url": { "type": "string" }
                  },
                  "required": ["FileMd5", "FileSize", "Url"]
                },
                "Voice": {
                  "type": "object",
                  "properties": {
                    "FileMd5": { "type": "string" },
                    "FileSize": { "type": "integer" },
                    "Url": { "type": "string" }
                  },
                  "required": ["FileMd5", "FileSize", "Url"]
                }
              }
            }
          },
          "required": ["MsgHead", "MsgBody"]
        },
        "EventName": {
          "type": "string",
          "enum": ["ON_EVENT_FRIEND_NEW_MSG"]
        }
      },
      "required": ["EventData", "EventName"]
    },
    "CurrentQQ": { "type": "integer", "description": "Bot QQ号" }
  },
  "required": ["CurrentQQ", "CurrentPacket"]
}
